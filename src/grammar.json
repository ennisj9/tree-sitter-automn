{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "automn",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "REPEAT",
            "content": {
              "type": "SYMBOL",
              "name": "newline"
            }
          },
          {
            "type": "SYMBOL",
            "name": "_definition"
          }
        ]
      }
    },
    "_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "model"
        }
      ]
    },
    "model": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_model_body"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_model_body": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "indent"
        },
        {
          "type": "SYMBOL",
          "name": "_model_child_definition"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "newline"
              },
              {
                "type": "SYMBOL",
                "name": "_model_child_definition"
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "dedent"
        }
      ]
    },
    "_model_child_definition": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "field"
        }
      ]
    },
    "field": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_type_declaration"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_type_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "type"
        }
      ]
    },
    "type": {
      "type": "SYMBOL",
      "name": "_any_type"
    },
    "_any_type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "atomic_type"
        },
        {
          "type": "SYMBOL",
          "name": "inline_enum"
        },
        {
          "type": "SYMBOL",
          "name": "array_of_type"
        },
        {
          "type": "SYMBOL",
          "name": "type_union"
        }
      ]
    },
    "_non_union_type": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "atomic_type"
        },
        {
          "type": "SYMBOL",
          "name": "inline_enum"
        },
        {
          "type": "SYMBOL",
          "name": "array_of_type"
        }
      ]
    },
    "inline_enum": {
      "type": "PREC_LEFT",
      "value": 2,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "variant"
          },
          {
            "type": "REPEAT1",
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": ","
                },
                {
                  "type": "SYMBOL",
                  "name": "variant"
                }
              ]
            }
          }
        ]
      }
    },
    "array_of_type": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "SYMBOL",
          "name": "_any_type"
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "type_union": {
      "type": "PREC_LEFT",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_non_union_type"
          },
          {
            "type": "REPEAT1",
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "|"
                },
                {
                  "type": "SYMBOL",
                  "name": "_non_union_type"
                }
              ]
            }
          }
        ]
      }
    },
    "atomic_type": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_-]*"
    },
    "variant": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_-]*"
    },
    "name": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_-]*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "indent"
    },
    {
      "type": "SYMBOL",
      "name": "dedent"
    },
    {
      "type": "SYMBOL",
      "name": "newline"
    }
  ],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}